/* Partner Layer Style */

#partner_box {
position:absolute;
float:left;
border-right:2px solid #F5F5F5;
}

#partner_box #partner_arrow {
background:url('/images/partner_layer_arrow.gif') left top no-repeat;
position:absolute;
left:0px; top:60px;
width:28px; height:23px;
z-index:2;
}

#partner_box #shadow_ri {
background:url('/images/partner_layer_shadow_ri.gif') left top no-repeat;
position:absolute;
float:left;
top:0px;
width:2px; height:100%;
}

#partner_box #shadow_bo {
background-image: url('/images/partner_layer_shadow_bo.gif');
background-repeat: repeat;
position:absolute;
clear:both;
left:0px; bottom: 0px;
width:100%; height:2px;
border-bottom:0px solid #F5F5F5;
}

#partner_box #partner_layer {
background-color:#FFFFFF;
border:1px solid #C3C3C3;
padding:20px;
margin-left:27px;
float:left;
position:relative;
display:inline;
}

#partner_layer #partner_img {
float:left;
}

#partner_layer #partner_cont {
float:left;
padding-left:20px;
display:inline;
}

#partner_cont span {
display:block;
margin-bottom:10px;
font-family:tahoma, verdana, sans-serif;
font-size:12px;
color:#C5C1BF;
}

#partner_cont span.tit
{
display:inline;
margin: 0;
color:#222222;
}

#partner_cont a.blt_link_tit {
