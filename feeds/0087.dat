/*me2DAY privacy*/

* {font-weight: normal;font-style: normal;text-decoration: none;text-align: left;text-indent: 0;margin:0;padding:0;}
body{background: #f1f2f2; color: #444; border-top:4px solid #404040;font-family:"NanumGothic", "NanumGothicOTF", "맑은고딕", dotum, 'Malgun Gothic', 'Trebuchet MS', 'Lucida Grande', Tahoma, Helvetica, Arial, sans-serif;}
