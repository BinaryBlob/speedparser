img,fieldset{border:0;}
li{list-style:none;}
strong{font-weight: bold;}
pre{padding: 3%;border-width: 1px;border-style: dotted;overflow: auto;}
table{border-collapse:collapse;}
hr{visibility:hidden;position:absolute;}
input{font-family: "NanumGothic","NanumGothicOTF","맑은고딕",dotum,'Malgun Gothic','Trebuchet MS','Lucida Grande',Tahoma,Helvetica,Arial,sans-serif;overflow:visible;}
textarea, .preview_txt{font-family: "NanumGothic","NanumGothicOTF","맑은고딕",dotum,'Malgun Gothic','Trebuchet MS','Lucida Grande',Tahoma,Helvetica,Arial,sans-serif;}
