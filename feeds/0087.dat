font-size:12px;
color:#E65802;
text-decoration:none;
padding-left:20px;
}

#partner_cont a:hover {
text-decoration:underline;
}


/* partner icon change styles */
dl.post_image dt a {
        position:relative;
        width:46px; height:46px;
    display:block;
        outline-style:none;
}

dl.post_image dt div.click {
    background-color:#000;
        opacity: .6;
    filter: alpha(opacity=60);
    -moz-opacity: 0.6;
    width:44px; height:16px;
    display:block;
        position:absolute;
    bottom:1px; left:1px;
        text-align:center;
    color:#FFF;
    font: 85% Trebuchet,"Trebuchet MS",Verdana,Sans-Serif;
    font-size:8pt;
    text-indent: 0;
    line-height: 19px;
    letter-spacing:.1em;
    overflow: hidden;
}

dl.post_image dt a:hover div.click {
        display:none;
        background-color:transparent;
}

dl.post_image dt a:hover div.ico {
        background-color:#000;
        opacity: .6;
        filter: alpha(opacity=60);
        -moz-opacity: 0.6;
        width:44px; height:44px;
        display:block;
        cursor:pointer;
        position:absolute;
        left:1px; top:1px;
}

dl.post_image div.ico {
    background:#000 url("/images/partner/service_icon_default.gif") left top no-repeat;
}

dl.post_image dt div.video {
        background:#000 url("/images/partner/service_icon_video.gif") left top no-repeat;
}

dl.post_image dt div.photo {
        background:#000 url("/images/partner/service_icon_photo.gif") left top no-repeat;
}

dl.post_image dt div.music {
        background:#000 url("/images/partner/service_icon_music.gif") left top no-repeat;
}
dl.post_image dt div.me2photo {
        background:#000 url("/images/partner/service_icon_me2photo.gif") left top no-repeat;
}

dl.post_image dt div.RevU {
    background:#000 url("/images/partner/service_icon_info.gif") left top no-repeat;
}

dl.post_image dt div.pumfit {
    background:#000 url("/images/partner/service_icon_info.gif") left top no-repeat;
}

