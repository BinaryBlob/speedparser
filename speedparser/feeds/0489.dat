    <body onload="if (top != self) document.write('<body style=\'background:transparent;\'></body>');">
        <table height="100%" width="100%">
            <tr>
                <td align="center" valign="middle">
                    <h1 id="l10n_title">We're sorry</h1>
                    <div class="sorry" style="width:535px;">
                        <p id="l10n_sorry">
                            Our servers are over capacity and certain pages may be temporarily unavailable. We're working quickly to resolve the issue.
                        </p>
                    </div>
                </td>
            </tr>
        </table>
        
        <script type="text/javascript" language="javascript">var status_code = '503'</script>
        <script type="text/javascript" language="javascript" src="http://assets.tumblr.com/languages/errors.js"></script>
    </body>
